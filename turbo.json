{
	"$schema": "https://turborepo.com/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"envMode": "loose",
	"ui": "tui",
	"tasks": {
		"build": {
			"outputs": ["dist", ".vercel", ".nitro", ".output", ".tanstack"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"push": {
			"cache": false
		}
	}
}
